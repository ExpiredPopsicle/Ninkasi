// Anonymous function tests.

// Call inline function directly.
function() {
    print("ASDFmdclkmdlksdclksdls,dcl;,\n");
}();

// Should do nothing.
1 + 2 + 3;

// Anonymous function assigned to variable and called.
var test = function() { print("HOLY SHIT\n"); };
test();

// Should do nothing.
1 + 2 + 3;

// Call an inline function directly.
function() {
    print("ASDF\n");
}();

// Normal function declaration.
function foo()
{
    print("ASDF\n");
}

// Anonymous function assigned to variable (equivalent to above).
var foo2 = function() {
};
